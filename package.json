{
  "name": "jorer",
  "private": true,
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "electron ./src/server/main",
    "buildp": "npm run gulp -- execute ---task=webpack ---profile=build-provider",
    "buildc": "npm run gulp -- execute ---task=webpack ---profile=build-client",
    "resource": "npm run gulp -- execute ---task=resource",
    "start-hot": "npm run gulp -- execute ---task=resource,resource:w,webpack-server ---profile=build-client",
    "start-dev": "concurrently \"electron ./src/server/main\" \"npm run start-hot\"",
    "build": "electron-packager . --out=dist --asar --overwrite --all",
    "gulp": "gulp"
  },
  "dependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.18.2",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "drivelist": "^5.0.3",
    "electron": "^1.4.8",
    "electron-builder": "^8.6.0",
    "electron-debug": "^1.1.0",
    "electron-devtools-installer": "^2.0.1",
    "electron-packager": "^8.3.0",
    "interact.js": "^1.2.6",
    "log4js": "^1.0.1",
    "material-ui": "^0.16.4",
    "react": "^15.4.0",
    "react-dom": "^15.4.0",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "react-tap-event-plugin": "^2.0.1",
    "redis": "^2.6.3",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "babel-loader": "^6.2.8",
    "concurrently": "^3.1.0",
    "css-loader": "^0.26.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-batch": "^1.0.5",
    "gulp-cached": "^1.1.1",
    "gulp-clean": "^0.3.2",
    "gulp-filter": "^4.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-util": "^3.0.7",
    "gulp-watch": "^4.3.11",
    "happypack": "^3.0.0",
    "imports-loader": "^0.6.5",
    "node-sass": "^3.13.0",
    "postcss-loader": "^1.1.1",
    "run-sequence": "^1.2.2",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^2.1.0-beta.27",
    "webpack-dev-server": "^2.1.0-beta.11",
    "webpack-target-electron-renderer": "^0.4.0"
  }
}
